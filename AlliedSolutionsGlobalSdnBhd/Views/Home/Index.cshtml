@model IEnumerable<AlliedSolutionsGlobalSdnBhd.Models.EqpReservationsDTO>

<div class="text-center">
    <h1 class="display-4">Table EqpReservations</h1>
</div>

<script>
  // Inline JavaScript to set table header background color
  document.querySelectorAll('th').forEach(function(th) {
    th.style.backgroundColor = '#007bff'; // Standard blue header color
    th.style.color = '#fff'; // Text color white
  });
</script>

@if (Model.Count() > 0)
{
    <table class="table table-bordered" >
        <thead class="thead-dark">
            <tr>
                <th>
                    No.
                </th>
                <th>
                    Book No
                </th>
                <th>
                    Record Source
                </th>
                <th>
                    Equipment
                </th>
                <th>
                    Act State
                </th>
                <th>
                    Act Time
                </th>
                <th>
                    Deact Time
                </th>
                <th>
                    Card Enable
                </th>
                <th>
                    Init
                </th>
            </tr>
        </thead>

        <tbody>
            @{
                int i = 1;
            }
            @foreach (var EqpReservations in Model)
            {

                <tr>
                    <td>
                        @i
                    </td>
                    <td>
                        @EqpReservations.BookSNo
                    </td>
                    <td>
                        @EqpReservations.RecordSource
                    </td>
                    <td>
                        @EqpReservations.Equipment
                    </td>
                    <td>
                        @EqpReservations.ActState
                    </td>
                    <td>
                        @EqpReservations.ActTime
                    </td>
                    <td>
                        @EqpReservations.DeactTime
                    </td>
                    <td>
                        @EqpReservations.CardEnable
                    </td>
                    <td>
                        @EqpReservations.Init
                    </td>
                </tr>
                i++;
            }

            </tbody>
    </table>

}

else
{
            <p>
                Not Exist
            </p>
}
